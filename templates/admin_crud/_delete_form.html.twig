<form method="post" action="{{ path('app_admin_crud_delete', {'category' : typeModule, 'id': object.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ object.id) }}">
	<button class="btn">Delete</button>
</form>
